{
  "name": "zeta",
  "version": "0.0.1",
  "description": "Zeta Function Fun",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r -f dist",
    "start": "yarn run web",
    "start-debug": "concurrently -k \"yarn run web-debug\" \"yarn run webpack\"",
    "build": "npm run clean && npm run build-structure && concurrently \"npm run build-root\" \"npm run build-server\" \"npm run build-css\" \"gulp deploy\" \"npm run copy-static-files\" \"npm run webpack:prod\"",
    "build-structure": "mkdir dist && cd dist && mkdir server && cd server && mkdir web && cd web && mkdir public && cd public && mkdir css",
    "build-css": "node-sass server/web/public/css/app.scss > dist/server/web/public/css/main.css && csso dist/server/web/public/css/main.css dist/server/web/public/css/main.css",
    "build-root": "babel index.js web.js crons.js workers.js vid-workers.js vid-index.js -d dist",
    "build-server": "babel server --x \".js\" --ignore server/web/public/js/*,server/web/views/*.pug -d dist/server",
    "copy-static-files": "cp package.json dist/package.json && cp -R server/web/public/js dist/server/web/public && cp -R server/web/public/css/*.css dist/server/web/public/css && cp -R server/web/public/images dist/server/web/public/images && cp server/web/public/favicon.ico dist/server/web/public/favicon.ico",
    "test": "nf -e test.env run -- ava",
    "test-watch": "nf -e test.env run -- ava -w -v",
    "prepush": "yarn lint && yarn test && yarn flow && yarn puglint && yarn format-check",
    "puglint": "pug-lint server/web/views",
    "format": "prettier --write \"**/*.{js,jsx,scss}\"",
    "format-check": "prettier -l \"**/*.{js,jsx,scss}\"",
    "flow": "flow check",
    "lint": "eslint --ext .js,.jsx .",
    "migrate": "nf run -- mariner migrate up",
    "rollback": "nf run -- mariner migrate down",
    "add-migration": "bash dev-ops/add-migration",
    "web": "nf run -- babel-watch -L index.js web",
    "web-debug-break": "nf run -- babel-watch --inspect-brk index.js web",
    "web-debug": "nf run -- babel-watch --inspect index.js web",
    "workers": "nf run -- babel-watch index.js workers",
    "workers-debug": "nf run -- node --inspect-brk index.js workers",
    "crons": "nf run -- babel-watch index.js cron",
    "crons-debug": "nf run -- node --inspect-brk index.js cron",
    "version-major": "bash dev-ops/version major",
    "version-minor": "bash dev-ops/version minor",
    "version-patch": "bash dev-ops/version patch",
    "release-candidate-major": "bash dev-ops/rc major",
    "release-candidate-minor": "bash dev-ops/rc minor",
    "release-candidate-patch": "bash dev-ops/rc patch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Mike Bondi",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-watch": "^2.0.7",
    "bluebird": "^3.5.0",
    "commander": "^2.9.0",
    "distraught": "^4.5.1",
    "lodash": "^4.17.1",
    "mariner": "^1.0.2",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.14",
    "newrelic": "^4.7.0",
    "node-sass": "^4.9.0",
    "pug": "^2.0.0-rc.3",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "ava": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-register": "^6.26.0",
    "cheerio": "^0.22.0",
    "concurrently": "^3.5.0",
    "csso": "^2.3.1",
    "eslint": "^4.19.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.80.0",
    "foreman": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-css": "^0.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-pug": "^3.3.0",
    "gulp-replace": "^0.6.1",
    "mock-knex": "^0.3.9",
    "postcss": "^6.0.13",
    "prettier": "1.13.5",
    "pug-lint": "^2.4.0",
    "semver": "^5.4.1",
    "source-map-support": "^0.5.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "ava": {
    "files": [
      "./test/templates/*-test.js",
      "./test/server/*-test.js"
    ],
    "concurrency": 3,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": false,
    "compileEnhancements": false,
    "sources": [
      "**/*.{js,pug}"
    ]
  },
  "engines": {
    "node": "^8.*"
  }
}
